{
  "name": "setupkit-ui-frontend",
  "version": "1.0.0",
  "description": "Frontend for SetupKit UI installer",
  "scripts": {
    "build": "npm run copy-to-dist",
    "copy-to-dist": "node -e \"const fs=require('fs');const path=require('path');if(!fs.existsSync('dist')){fs.mkdirSync('dist');}['index.html','style.css','app.js'].forEach(f=>{fs.copyFileSync(path.join('src',f),path.join('dist',f));});\""
  },
  "author": "",
  "license": "MIT"
}
